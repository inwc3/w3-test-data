    call TriggerRegisterUnitEvent(deathTrigger, bear3, EVENT_UNIT_DEATH)
    
    call TimerStart(CreateTimer(), 0.01, true, function updateBolts)
    
    call UnitAddAbility(kael, 'Aeth')
    
    call ForGroup(caravan, function shareSight)
    
